// User function Template for Java

class Solution {
    static int isPossible(int N, int arr[]) {
        // code here
         long sum = 0;
        for (int i = 0; i < N; i++) {
            int num = arr[i];
            while (num > 0) {
                sum += num % 10;
                num /= 10;
            }
        }
        return sum % 3 == 0 ? 1 : 0;
    }
}
