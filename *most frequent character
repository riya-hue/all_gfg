class Solution {
    public static char getMaxOccuringChar(String s) {
        int[] freq = new int[26];
        for (char c : s.toCharArray()) {
            freq[c - 'a']++;
        }
        
        char maxChar = 'a';
        int maxCount = 0;
        
        for (int i = 0; i < 26; i++) {
            if (freq[i] > maxCount) {
                maxCount = freq[i];
                maxChar = (char) (i + 'a');
            }
        }
        
        return maxChar;
    }
}
