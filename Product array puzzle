
class Solution {
    public static int[] productExceptSelf(int arr[]) {
        // code 
        int n=arr.length;
        int[] res=new int[n];
        
        //calculate prefix products and store in res
        int prefix=1;
        for(int i=0;i<n;i++){
            res[i]=prefix;
            prefix*=arr[i];
        }
         //calculate suffix products and store in res
        int suffix=1;
        for(int i=n-1;i>=0;i--){
            res[i]*=suffix;
            suffix*=arr[i];
        }
        return res;
        
    }
}
